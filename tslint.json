{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "rules": {
P    "quotemark": [true, "single", "jsx-double", "avoid-escape"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "jsx-no-lambda": false,
    "import-name": [true, {
      "choices.js": "Choices"
    }]
  }
}
